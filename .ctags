--links=no
--excmd=number
--recurse=yes

--langdef=customjs
--langmap=customjs:.js.cjs.mjs.ts.vue
--regex-customjs=/^[ \t]*(export[ \t]+)?(default[ \t]+)?(abstract[ \t]+)?class[ \t]+([a-zA-Z0-9_]+)/\4/c,classes/
--regex-customjs=/^[ \t]*(export[ \t]+)?(namespace|module)[ \t]+([a-zA-Z0-9_]+)/\3/n,modules/
--regex-customjs=/^[ \t]*(export[ \t]+)?type[ \t]+([a-zA-Z0-9_]+)/\2/t,types/
--regex-customjs=/^[ \t]*(export[ \t]+)?(var|const|let)[ \t]+([a-zA-Z0-9_]+)/\3/v,variables/
--regex-customjs=/^[ \t]*(export[ \t]+)?(async[ \t]+)?function[ \t]+([a-zA-Z0-9_]+)/\3/f,functions/
--regex-customjs=/^[ \t]*(export[ \t]+)?interface[ \t]+([a-zA-Z0-9_]+)/\2/i,interfaces/
--regex-customjs=/^[ \t]*(export[ \t]+)?enum[ \t]+([a-zA-Z0-9_]+)/\2/e,enums/
--regex-customjs=/^[ \t]*(public|private|protected)?[ \t]*(static)?[ \t]*(readonly)?[ \t]*(async)?[ \t]*(#[a-zA-Z0-9_]+)[ \t]*\(/\5/m,methods/
--regex-customjs=/^[ \t]*(public|private|protected)?[ \t]*(static)?[ \t]*(readonly)?[ \t]*(async)?[ \t]*(override)?[ \t]*([a-zA-Z][a-zA-Z0-9_]*)[ \t]*\([^)]*\)[ \t]*[:{]/\6/m,methods/
--regex-customjs=/^[ \t]*(public|private|protected)?[ \t]*(static)?[ \t]*(readonly)?[ \t]*(#[a-zA-Z0-9_]+)[ \t]*[:=]/\4/p,properties/
--regex-customjs=/^[ \t]*(public|private|protected)?[ \t]*(static)?[ \t]*(readonly)?[ \t]*([a-zA-Z][a-zA-Z0-9_]*)[ \t]*[:=][ \t]*[^(]/\4/p,properties/
