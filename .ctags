--langdef=customjs
--langmap=customjs:.js.cjs.mjs.ts.vue
--regex-customjs=/^[ \t]*(export)?[ \t]*(abstract)?class[ \t]+([a-zA-Z0-9_]+)/\2/c,classes/
--regex-customjs=/^[ \t]*(export)?[ \t]*(namespace|module)[ \t]+([a-zA-Z0-9_]+)/\2/n,modules/
--regex-customjs=/^[ \t]*(export)?[ \t]*type[ \t]+([a-zA-Z0-9_]+)/\2/t,types/
--regex-customjs=/^[ \t]*export[ \t]+(var|const|let)[ \t]+([a-zA-Z0-9_]+)/\1/v,variables/
--regex-customjs=/^[ \t]*var[ \t]+([a-zA-Z0-9_]+)[ \t]*=[ \t]*function[ \t]*\(\)/\1/l,varlambdas/
--regex-customjs=/^[ \t]*(export[ \t]+)?(async[ \t]+)?function[ \t]+([a-zA-Z0-9_]+)[ \t]*\(/\3/f,functions/
--regex-customjs=/^[ \t]*(public|private|protected|static|readonly|abstract|async|override)*[ \t]*([a-zA-Z0-9_]+)[ \t]*\([^)]*\)[ \t]*[:{]/\2/m,methods/
--regex-customjs=/^[ \t]*(export)?[ \t]*interface[ \t]+([a-zA-Z0-9_]+)/\2/i,interfaces/
--regex-customjs=/^[ \t]*(export)?[ \t]*enum[ \t]+([a-zA-Z0-9_]+)/\2/e,enums/
